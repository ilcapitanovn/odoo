<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Sequence -->
        <record id="freight_trading_booking_sequence" model="ir.sequence">
            <field name="name">Freight Trading Booking Sequence</field>
            <field name="code">freight.trading.booking.sequence</field>
            <field name="prefix">TRA%(month)s%(y)s-</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="use_date_range">True</field>
            <field name="company_id" />
            <!-- TODO: Remember to install module OCA sequence_reset-period before enable this field -->
<!--            <field name="range_reset">monthly</field>-->
        </record>
    </data>
</odoo>