<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Sequence -->
        <record id="freight_langson_booking_sequence" model="ir.sequence">
            <field name="name">Freight LangSon Booking Sequence</field>
            <field name="code">freight.langson.booking.sequence</field>
            <field name="prefix">CT-VTLS</field>  <!-- Format will append year in codes -->
